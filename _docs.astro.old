})
export class AppComponent implements OnInit {
  
  ngOnInit() {
    const script = document.createElement('script');
    script.defer = true;
    script.src = 'https://your-domain.com/api/v1/sdk.js';
    document.head.appendChild(script);
  }
}`,

	django: `<!-- templates/base.html -->
